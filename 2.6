#include <iostream>
using namespace std;
int main() {
    int ay;
    cout << "Ay nomresini daxil edin (1-12): ";
    cin >> ay;
    if (ay == 12 || ay == 1 || ay == 2) {
        cout << "Qis fesli";
    }
    else if (ay >= 3 && ay <= 5) {
        cout << "Yaz fesli";
    }
    else if (ay >= 6 && ay <= 8) {
        cout << "Yay fesli";
    }
    else if (ay >= 9 && ay <= 11) {
        cout << "Payiz fesli";
    }
    else {
        cout << "Yanlis ay nomresi.";
    }
    return 0;
}
